    "@vitejs/plugin-vue": "latest",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "cssnano": "^5.1.15",
    "cz-git": "^1.5.1",
